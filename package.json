{
  "name": "test-app",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "start": "env-cmd -e default,development vite --host",
    "build": "tsc && env-cmd -e default,production vite build",
    "build:staging": "tsc && env-cmd -e default,staging vite build",
    "build:production": "tsc && env-cmd -e default,production vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "coverage": "vitest run --coverage",
    "test": "vitest"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.7.0",
    "qs": "^6.14.0",
    "react": "^19.0.0",
